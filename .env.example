# Server: your JoulePAI API key (used to verify payments)
JOULEPAI_API_KEY=jlp_your-api-key-here
# Server: your wallet handle (where payments are sent)
JOULEPAI_HANDLE=@your-handle

# Client only: wallet ID of the paying agent (for client-example.js)
JOULEPAI_CLIENT_WALLET_ID=your-wallet-uuid-here

# Server port
PORT=3000
